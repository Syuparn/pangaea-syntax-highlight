{
    "name": "pangaea-syntax-highlight",
    "displayName": "Pangaea Syntax Highlight",
    "description": "Syntax highlighter for Pangaea programming language",
    "publisher": "Syuparn",
    "icon": "images/pangaroo.png",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.45.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Syuparn/pangaea-syntax-highlight.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pangaea",
                "aliases": [
                    "Pangaea",
                    "pangaea"
                ],
                "extensions": [
                    ".pangaea"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pangaea",
                "scopeName": "source.pangaea",
                "path": "./syntaxes/pangaea.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "pangaea",
                "path": "./snippets/pangaea.json"
            }
        ]
    }
}
